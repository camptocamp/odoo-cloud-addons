<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_attachment_form"
        model="ir.ui.view">
        <field name="name">ir.attachment</field>
        <field name="model">ir.attachment</field>
        <field name="inherit_id"
            ref="base.view_attachment_form" />
        <field name="arch"
            type="xml">
            <field name="mimetype"
                position="after">
                <field name="to_delete" />
            </field>
        </field>
    </record>

    <record id="view_attachment_search"
        model="ir.ui.view">
        <field name="model">ir.attachment</field>
        <field name="inherit_id"
            ref="base.view_attachment_search" />
        <field name="arch"
            type="xml">
            <field name="name"
                position="after">
                <field name="to_delete" />
            </field>
            <xpath expr="//separator[last()]" position="before">
                <filter name="to_delete" string="To Delete"/>
            </xpath>
        </field>
    </record>

</odoo>